{
  "name": "@agentic-sdlc/product-owner-lambda",
  "version": "1.0.0",
  "description": "Product Owner Lambda Agent - Generates application requirements using DDD principles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
