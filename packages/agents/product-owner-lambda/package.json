    "zod": "^3.22.4",
    "pino": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "axios": "^1.6.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "product-owner",
